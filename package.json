{
  "name": "botanist",
  "version": "1.0.0",
  "description": "Silly Slack Bot",
  "main": "src/main.js",
  "scripts": {
    "startdev": "SET NODE_ENV=development & node --nolazy --harmony src/Main.js",
    "startdevl": "sudo NODE_ENV=development node --nolazy --harmony src/Main.js",
